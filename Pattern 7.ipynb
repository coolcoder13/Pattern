   ],
   "source": [
    "def ff(n):\n",
    "    s=n+1\n",
    "    for i in range(n-1):\n",
    "        print(' '*i,i+1,' '*s,i+1,' '*i)\n",
    "        s-=2\n",
    "    j=4\n",
    "    print(' '*j,j,' '*s,' '*j)\n",
    "    s=n-3\n",
    "    for i in range(n-1,0,-1):\n",
    "        print(' '*(i-1),i,' '*s,i,' '*(i+1))\n",
    "        s+=2\n",
    "        \n",
    "ff(4)"
   ]
